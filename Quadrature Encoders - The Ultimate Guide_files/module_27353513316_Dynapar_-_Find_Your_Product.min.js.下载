var module_27353513316=function(){var i={};i18n_getmessage=function(){return hs_i18n_getMessage(i,hsVars.language,arguments)},i18n_getlanguage=function(){return hsVars.language},$(document).ready((function(){const i=$(".has-clickable-logic");i.find(".choice").click((function(){let n=[];const t=$(this);t.addClass("active").siblings().removeClass("active");const e=t.find("h3").text();t.parent().siblings(".question").each((function(){$(this).attr("data-filter")===e&&$(this).show().siblings(".question").hide()})),i.find(".active:visible").each((function(){n.push($(this).find("h3").text())})),$(".product-name").text(n.join(" "))}))}))}();